{
    "window.zoomLevel": -0.5,
    "window.title": "${dirty}${rootName}${separator}${activeEditorShort}",
    "editor.fontFamily": "Consolas",
    "editor.fontSize": 12,
    "editor.lineHeight": 0,
    "editor.lineNumbers": "on",
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.suggestSelection": "first",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.suggest.showStatusBar": true,
    "diffEditor.wordWrap": "on",
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#FFD700",
        "editorBracketHighlight.foreground2": "#DA70D6",
        "editorBracketHighlight.foreground3": "#87CEFA"
    },
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.preferredDarkColorTheme": "Atom One Dark",
    "workbench.colorTheme": "Aura Dark",
    "workbench.iconTheme": "eq-material-theme-icons-palenight",
    "workbench.editorAssociations": {
        "*.ebu": "hexEditor.hexedit",
        "*.ebc": "hexEditor.hexedit",
        "*.ebcc": "hexEditor.hexedit"
    },
    "screencastMode.fontSize": 32,
    "screencastMode.mouseIndicatorSize": 32,
    "terminal.integrated.fontFamily": "Monaco",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "explorer.compactFolders": false,
    "explorer.openEditors.visible": 0,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        "*.ccjs": "javascript",
        ".clang-format": "plaintext",
        "*.ebu": "plaintext",
        "*.ebf": "plaintext",
        "*.ebc": "plaintext",
        "*.pgm": "plaintext",
        "*.ebcc": "plaintext"
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.format.semicolons": "insert",
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "typescript.format.semicolons": "insert",
    "C_Cpp.clang_format_path": "/opt/homebrew/bin/clang-format",
    "C_Cpp.clang_format_style": "file:/Users/george/.format/.clang-format",
    "java.format.settings.url": "~/.format/java-format-config.xml",
    "java.format.settings.profile": "style",
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "sql-formatter.dialect": "sql",
    "sql-formatter.linesBetweenQueries": 1
}